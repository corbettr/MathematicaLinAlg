   3.65134345510728*^9}},ExpressionUUID->"00b090c7-22e0-468d-8a75-\
3d85c3eafcbe"],

Cell[BoxData[{
 RowBox[{"a1", " ", "=", " ", 
  RowBox[{
   RowBox[{"(", 
    RowBox[{"14", " ", "+", " ", 
     RowBox[{"Sqrt", "[", 
      RowBox[{
       RowBox[{"14", "^", "2"}], " ", "-", " ", 
       RowBox[{
        RowBox[{"(", "4", ")"}], 
        RowBox[{"(", "3", ")"}], 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"-", "174"}], "Pi"}], ")"}]}]}], "]"}]}], ")"}], "/", 
   RowBox[{"(", 
    RowBox[{"2", 
     RowBox[{"(", "3", ")"}]}], ")"}]}]}], "\[IndentingNewLine]", 
 RowBox[{"a2", " ", "=", " ", 
  RowBox[{
   RowBox[{"(", 
    RowBox[{"14", " ", "-", " ", 
     RowBox[{"Sqrt", "[", 
      RowBox[{
       RowBox[{"14", "^", "2"}], " ", "-", " ", 
       RowBox[{
        RowBox[{"(", "4", ")"}], 
        RowBox[{"(", "3", ")"}], 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"-", "174"}], "Pi"}], ")"}]}]}], "]"}]}], ")"}], "/", 
   RowBox[{"(", 
    RowBox[{"2", 
     RowBox[{"(", "3", ")"}]}], ")"}]}]}]}], "Input",
 CellChangeTimes->{{3.651340452639648*^9, 3.651340456904345*^9}, {
